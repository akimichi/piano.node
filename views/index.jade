extends layout

block head
  meta(charset='utf-8')
  title= title
  link(rel='stylesheet', href='/css/style.css')
  block scripts
    script(type='text/javascript', src="/js/index.js")
block content
  h2 音色 
    br
    form#submit_form(method="POST", action="/api/channel/voice/program/change")
      select#programs
        each program, i in programChange
          option(value=i+1) #{programChange[i]}
  h2 ピッチ 
    #slider-vertical-pitch

  h2 モジュレーション 
    #slider-vertical-modulation

